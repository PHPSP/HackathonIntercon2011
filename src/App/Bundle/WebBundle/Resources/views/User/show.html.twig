<h1>User</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Twitterid</th>
            <td>{{ entity.twitterId }}</td>
        </tr>
        <tr>
            <th>Screenname</th>
            <td>{{ entity.screenName }}</td>
        </tr>
        <tr>
            <th>Accesstoken</th>
            <td>{{ entity.accessToken }}</td>
        </tr>
        <tr>
            <th>Lomadeeid</th>
            <td>{{ entity.lomadeeId }}</td>
        </tr>
        <tr>
            <th>Frequency</th>
            <td>{{ entity.frequency }}</td>
        </tr>
        <tr>
            <th>Fallback</th>
            <td>{{ entity.fallback }}</td>
        </tr>
        <tr>
            <th>Hashtag</th>
            <td>{{ entity.hashtag }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('user') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('user_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('user_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
